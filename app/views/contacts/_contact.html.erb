<li data-id="<%= contact.id %>">
  <span class="<%= "obsolete" if contact.state == -1 %>"><%= "#{contact.first_name} #{contact.last_name} email: #{contact.email} skype: #{contact.skype}" %></span>
  <%= link_to "Delete", contact_path(contact), :method => :delete, :confirm => "Are you sure?", :remote => true %>
  <% if contact.state == -1 %>
    <%= link_to "Actual", contact_path(contact, :state => 0), :method => :put, :remote => true %>
  <% else %>
    <%= link_to "Obsolete", contact_path(contact, :state => -1), :method => :put, :remote => true %>
  <% end %>
</li>
